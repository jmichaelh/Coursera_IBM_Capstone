    "data = requests.get('http://www.parisnet.com/parismap.html')\n",
    "src = data.content\n",
    "soup = BeautifulSoup(src, 'html.parser')\n",
    "\n",
    "arr = soup.find_all('h3')\n",
    "\n",
    "data = [x.text.split(';')[-1].strip() for x in arr]\n",
    "df = pd.DataFrame(data, columns=['Arrond'])\n",
    "\n",
    "df['Number'] = df['Arrond'].str.split('-').str[0]\n",
    "df['Arrond'] = df['Arrond'].str.split('-').str[1]\n",
    "df = df.drop(0).drop(22).drop(23).drop(24).drop(25).drop(21)\n",
    "df"
   ]
